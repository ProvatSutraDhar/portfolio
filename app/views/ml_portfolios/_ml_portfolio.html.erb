    <p class="h2">
      <strong>Title:</strong>
      <%= ml_portfolio.title %>
    </p>
  </div>
  <div class="card-body text-success">
    <p>
      <strong>Subtitle:</strong>
      <%= ml_portfolio.subtitle %>
    </p>

    <p>
      <strong>Type:</strong>
      <%= ml_portfolio.portfolio_type %>
